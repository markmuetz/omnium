#!/usr/bin/env bash
if ${OMNI_DEBUG:-false}; then
    set -x
fi

OMNI=$(basename $0)
OMNI_BIN_DIR=$(dirname $0)
OMNI_HOME=$(dirname $OMNI_BIN_DIR)
OMNI_PYTHONPATH="$OMNI_HOME"
OMNI_COMMAND="python $OMNI_PYTHONPATH/omnium/omnium_cmd.py"

export OMNI_HOME=$OMNI_HOME
export PYTHONPATH=$OMNI_PYTHONPATH

exec $OMNI_COMMAND "$@"
